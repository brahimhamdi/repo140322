[program:{{ item.key }}]
command=/etc/alternatives/java  -Dlogging.path=/var/log/spring/ -jar {{ remote_folder }}/{{ item.value.artifactId }}
autostart=true
autorestart=true
startsecs=10
startretries=3
stdout_logfile=/var/log/spring/%(program_name)-stdout.log
stderr_logfile=/var/log/spring/%(program_name)-stderr.log